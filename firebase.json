{
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    "rewrites": [{
      "source": "/render",
      "function": "render"
    }, {
      "source": "/version",
      "function": "version"
    }, {
      "source": "/screenshot",
      "function": "screenshot"
    }, {
      "source": "/test",
      "function": "test"
    }],
    "ignore": [
      "firebase.json",
      "**/.md",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "source": "functions"
  }
}